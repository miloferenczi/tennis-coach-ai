{
"metadata": {
"version": "1.1",
"sport": "tennis",
"description": "Machine-readable diagnostic decision tree for tennis stroke analysis tuned for elite coaching standards",
"metricsSchema": {
"preparationTime": "s from opponent impact to racquet set (unitless normalization optional)",
"velocity": "normalized racquet head speed (0–1) or m/s if calibrated",
"acceleration": "normalized racquet head acceleration (0–1) or m/s²",
"rotation": "degrees of trunk rotation from unit turn to contact",
"hipShoulderSeparation": "degrees hips vs shoulders at max coil",
"contactPointVariance": "std dev (m) of 3D contact location over last N strokes",
"forwardMomentum": "player COM forward velocity (m/s) or normalized 0–1",
"backFootWeightAtContact": "fraction of weight on back foot at contact (0–1)",
"elbowAngleAtContact": "degrees",
"armExtension": "normalized 0–1 (1 = full reach on intended pattern)",
"swingPathSmoothness": "0–100 (higher = smoother jerk-minimized path)"
}
},
"issuePriority": {
"foundation": ["latePreparation", "inconsistentContactPoint"],
"power": ["lowRacquetSpeed", "insufficientRotation", "poorWeightTransfer"],
"refinement": ["collapsingElbowChickenWing", "closedStanceLimitingPower", "poorFollowThrough"]
},
